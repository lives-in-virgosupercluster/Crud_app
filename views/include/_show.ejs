<table class="table">
    <thead class="thead-dark">
       
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>@Email</th>
            <th>Gender</th>
            <th>Status</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% for(var i=0;i<users.length;i++){%>
        <tr>
            <td>
                <%=i+1%>
            </td>
            <td>
              <%=users[i].name%>
            </td>
            <td>
                <%=users[i].email%>
            </td>
            <td>
                <%=users[i].gender%>
            </td>
            <td>
                <%=users[i].status%>

            </td>
            <td>
                <a href="/update-user?id=<%=users[i]._id%>" class="btn border-shadow update">
                    <span class="text-gradient"><i class="fa-solid fa-pencil"></i></span>
                </a>
                <a  class="btn border-shadow delete" data-id=<%= users[i]._id%>>
                    <span class="text-gradient"><i class="fa-sharp fa-solid fa-trash"></i></span>
                </a>
            </td>
        </tr>
        <%}%>
    </tbody>

</table>